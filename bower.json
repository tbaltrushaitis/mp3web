{   "name":         "mp3web"
  , "title":        "Web MP3 Player and Media Library Manager"
  , "description":  "Web MP3 Player and Media Library Manager based on Node.js, Laravel and HTML5 + lightweight JavaScript MVC modeling on FrontEnd"
  , "version":      "1.0.3"
  , "author": [
      "tbaltrushaitis@gmail.com"
    ]
  , "license":  "MIT"
  , "private":  false
  , "homepage": "https://tbaltrushaitis.github.io/mp3web/"
  , "main":     ""
  , "keywords": [
        "mp3"
      , "audio"
      , "player"
      , "laravel"
      , "media"
      , "library"
      , "storage"
      , "manager"
      , "web"
    ]
  , "moduleType": [
      "project"
    ]
  , "ignore": [
        "**/.*"
      , "animation"
      , "build"
      , "bower_components"
      , "bower_modules"
      , "dist"
      , "docs"
      , "engine"
      , "laravel-*"
      , "node_modules"
      , "test"
      , "tests"
      , "webroot"
      , "web-*"
    ]
  , "dependencies": {
        "animate.css":          "^4.1.1"
      , "bootstrap":            "~3.4.1"
      , "bootstrap-tagsinput":  "~0.8.0"
      , "font-awesome":         "#6.2.0"
      , "html5shiv":            "~3.7.3"
      , "jquery":               "3.7.1"
      , "jquery-tmpl":          "#b504c8afba"
      , "lato-font":            "~3.0.0"
      , "lodash":               "~4.17.21"
      , "normalize.css":        "~8.0.0"
      , "noty":                 "~2.4.1"
      , "raty":                 "~2.8.0"
      , "requirejs":            "~2.3.6"
      , "respond":              "~1.4.2"
      , "roboto-webfont-bower": "roboto-webfont#~0.1.1"
    }
  , "devDependencies": {
    }
  , "resolutions": {
      "jquery": "3.5.1"
    }
  , "disabledDependencies": {
        "admin-lte": "~2.4.8"
      , "underscore": "~1.9.1"
    }
  , "outdatedDependencies": {
      "bxslider-4":   "bx-slider.js#^4.2.12"
    }
  , "overrides": {
      "admin-lte": {
        "main": [
            "dist/**/*.*"
          , "!**/index2.html"
          , "!**/*.less"
        ]
      }
    , "bootstrap": {
        "main": [
            "dist/**/*.*"
          , "!**/*.less"
          , "!**/npm.js"
          , "!**/*.min.js"
          , "!*-theme.*"
        ]
      }
    , "font-awesome": {
        "main": [
            "fonts/*"
          , "css/*.css"
          , "!css/*.min.css"
        ]
      }
    , "font-awesome-5": {
        "main": [
            "**/webfonts/*.*"
          , "**/css/*.css"
          , "!css/*.min.css"
        ]
      }
    , "jquery-tmpl": {
        "main": [
          "*.js"
        ]
      }
    , "lato-font": {
        "main": [
            "css/*.css"
          , "!css/*.min.css"
          , "!fonts/**"
        ]
      }
    , "raty": {
        "main": [
          "lib/**/*.*"
        ]
      }
    , "wdt-loading": {
        "main": [
            "wdtLoading.*"
          , "wdtLoading.js"
          , "wdtMarkup.html"
        ]
      }
    }
  , "overridesDisabled": {
      "lato-font": {
        "main": [
            "fonts/**"
          , "css/*.css"
          , "!css/*.min.css"
        ]
      }
    }
  , "group": {
      "fonts": [
          "font-awesome"
        , "font-awesome-5"
        , "lato-font"
        , "roboto-webfont-bower"
      ]
    , "libs": [
          "jquery"
        , "bootstrap"
        , "lodash"
        , "requirejs"
      ]
    , "plugins": [
          "bootstrap-tagsinput"
        , "html5shiv"
        , "jquery-tmpl"
        , "raty"
        , "noty"
      ]
    , "ui": [
          "animate.css"
        , "normalize.css"
      ]
    , "themes": [
        "admin-lte"
      ]
    , "front":     ["fonts", "libs", "plugins", "ui"]
    , "cabinet":   ["fonts", "libs", "plugins", "ui"]
    , "dashboard": ["fonts", "libs", "plugins", "ui", "themes"]
    }
}
