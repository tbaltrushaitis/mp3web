{
    "name":        "mp3web"
  , "title":       "Web MP3 Player and Media Library Manager"
  , "description": "Web MP3 Player and Media Library Manager based on Node.js, Laravel and HTML5 + lightweight JavaScript MVC modeling on FrontEnd"
  , "version":     "1.0.3"
  , "type":        "project"
  , "license":     "MIT"
  , "homepage":    "https://tbaltrushaitis.github.io/mp3web/"
  , "author": {
        "name":  "Baltrushaitis Tomas"
      , "email": "tbaltrushaitis@gmail.com"
      , "url":   "https://github.com/tbaltrushaitis"
    }
  , "authors": [
      {   "name":  "tbaltrushaitis"
        , "email": "tbaltrushaitis@gmail.com"
      }
    ]
  , "require": {
        "php":                  ">=7.0.0"
      , "fideloper/proxy":      "~3.3"
      , "laravel/framework":    "~5.5.49"
      , "laravel/tinker":       "~1.0"
    }
  , "require-disabled": {
        "php":                  ">=5.5.38"
      , "fideloper/proxy":      "~3.3"
      , "laravel/framework":    "~5.2.31"
      , "laravel/tinker":       "~1.0"
    }
  , "require-dev": {
        "phpunit/phpunit":      "~6.0"
      , "filp/whoops":          "~2.0"
      , "fzaninotto/faker":     "~1.4"
      , "mockery/mockery":      "~1.0"
      , "wisembly/elephant.io": "~3.0"
      , "symfony/thanks":       "^1.0"
    }
  , "require-dev-disabled": {
    }
  , "autoload": {
      "classmap": [
          "database/seeds"
        , "database/factories"
      ]
    , "psr-4": {
        "App\\": "app/"
      }
    }
  , "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    }
  , "extra": {
      "laravel": {
        "dont-discover": []
      }
    }
  , "scripts": {
      "post-root-package-install": [
        "@php -r \"file_exists('.env') || copy('.env.rc', '.env');\""
      ]
    , "post-create-project-cmd": [
          "@php artisan -v -n preset none"
        , "@php artisan key:generate"
      ]
    , "post-install-cmd": [
          "Illuminate\\Foundation\\ComposerScripts::postInstall"
        , "@php artisan --ansi -v -n vendor:publish"
        , "@php artisan --ansi -v -n migrate"
      ]
    , "post-update-cmd": [
          "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        , "@php artisan --ansi -v -n config:cache"
        , "@php artisan --ansi -v -n route:cache"
        , "@php artisan --ansi -v -n optimize"
        , "@php artisan -v -n migrate:status > .MIGRATE_STATUS"
        , "@php artisan -v -n route:list > .ROUTES_LIST"
        , "@php artisan -V > .ENGINE_VERSION"
        , "@php artisan env > .ENVIRONMENT"
        , "@php artisan inspire > .INSPIRATION"
      ]
    , "post-autoload-dump": [
        "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
      ]
    }
  , "config": {
        "preferred-install":    "dist"
      , "sort-packages":        true
      , "optimize-autoloader":  true
    }
  , "keywords": [
        "mp3"
      , "audio"
      , "player"
      , "laravel"
      , "media"
      , "library"
      , "storage"
      , "manager"
      , "web"
    ]
}
